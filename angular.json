{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lifenet-angular": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/lifenet-angular",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/style.css",
              "src/assets/css/style-responsive.css",
              "src/assets/css/vertical-rhythm.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/splitting.css",
              "src/assets/css/YTPlayer.css",
              "src/assets/css/demo-gradient/demo-gradient.css"
            ],
            "scripts": [
              "src/assets/js/jquery.min.js",
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/js/plugins.js",
              "src/assets/js/jquery.ajaxchimp.min.js",
              "src/assets/js/all.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "lifenet-angular:build:production"
            },
            "development": {
              "browserTarget": "lifenet-angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lifenet-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/style.css",
              "src/assets/css/style-responsive.css",
              "src/assets/css/vertical-rhythm.min.css",
              "src/assets/css/magnific-popup.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/splitting.css",
              "src/assets/css/YTPlayer.css",
              "src/assets/css/demo-gradient/demo-gradient.css"
            ],
            "scripts": [
              "src/assets/js/jquery.min.js",
              "src/assets/js/jquery.ajaxchimp.min.js",
              "src/assets/js/bootstrap.bundle.min.js",
              "src/assets/js/plugins.js",
              "src/assets/js/all.js"
            ]
          }
        }
      }
    }
  }
}